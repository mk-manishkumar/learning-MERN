<% include ./partials/header.ejs %>
<div class="w-full min-h-screen px-10 py-8">
  <div class="cards flex flex-wrap gap-10">
    <% user.posts.forEach(function(elem) { %>
    <div class="card w-32">
      <div class="w-full h-40 rounded-lg overflow-hidden">
        <img src="/images/uploads/<%= elem.image %>" alt="<%= elem.imageText %>" class="w-full h-full object-cover" />
      </div>
      <h5 class="mt-4 text-sm text-center"><%= elem.title %></h5>
    </div>
    <% }); %>
  </div>
</div>
<% include ./partials/footer.ejs %>
